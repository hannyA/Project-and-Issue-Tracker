<% layout('layouts/tickets-boilerplate') %>

<nav style="--bs-breadcrumb-divider: '>'" aria-label="breadcrumb">
  <ol class="breadcrumb">
    <li class="breadcrumb-item"><a href="/ticket">Tickets</a></li>
    <li class="breadcrumb-item active" aria-current="page">Create Ticket</li>
  </ol>
</nav>

<h1>Create new ticket</h1>

<form action="/ticket/" method="post">
  <div>
    <label for="title">Title</label>
    <input type="text" name="ticket[title]" id="title" />
  </div>

  <div>
    <label for="description">Description</label>
    <input type="text" name="ticket[description]" id="description" />
  </div>

  <button>Create</button>
</form>
