<% layout('layouts/admins-boilerplate') %>

<h1>Users</h1>
<a href="/admin/users/new">Add new User</a>
<div>
  <div>
    <div>
      <table class="table">
        <thead>
          <tr>
            <th scope="col">First Name</th>
            <th scope="col">Last Name</th>
            <th scope="col">Role</th>
            <th scope="col">Email</th>
            <th scope="col">DOB</th>
          </tr>
        </thead>
        <tbody>
          <% for (let user of users) { %>
          <tr>
            <td><%= user.first %></td>
            <td><%= user.last %></td>
            <td><%= user.role %></td>
            <td><%= user.email %></td>
            <td><%= user.dob %></td>
          </tr>

          <% }%>
        </tbody>
      </table>
    </div>
  </div>

  <footer class="mt-5"></footer>
</div>
